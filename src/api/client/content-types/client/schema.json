{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 1,
      "maxLength": 32,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "client"
    },
    "lastName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "minLength": 1,
      "maxLength": 32
    },
    "case_studies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case-study.case-study",
      "mappedBy": "client"
    }
  }
}
